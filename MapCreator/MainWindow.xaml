        <Canvas Name="MainCanvas" Background="#FF86CB7B" MouseLeftButtonDown="MainCanvas_MouseLeftButtonDown" MouseLeftButtonUp="MainCanvas_MouseLeftButtonUp" MouseRightButtonDown="MainCanvas_MouseRightButtonDown" MouseRightButtonUp="MainCanvas_MouseRightButtonUp" MouseMove="MainCanvas_MouseMove">
            
        </Canvas>
    </Grid>
</Window>
